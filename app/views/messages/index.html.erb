<h1>telegram message</h1>
<p>아래에 메세지 입력하세요</p>
<form action="/messages/send_msg">
  message :  <input type="text" name="msg">
  <input type="submit">
</form>
<% @messages.each do |message| %>
<p>
  <%= message.user.email %>
  <%= message.content %> |
  <%= time_ago_in_words(message.created_at).split(" ")[0] + "분전" %></p>
<% end %>
